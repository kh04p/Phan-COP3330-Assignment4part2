<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.saveListController">
   <children>
      <Label layoutX="20.0" layoutY="10.0" prefHeight="37.0" prefWidth="360.0" text="Please enter the name of the file you want to save:" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font name="Calibri Italic" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="fileNameField" layoutX="20.0" layoutY="50.0" prefHeight="27.0" prefWidth="327.0" promptText="Name">
         <font>
            <Font name="Calibri" size="14.0" />
         </font></TextField>
      <Button layoutX="358.0" layoutY="50.0" mnemonicParsing="false" onAction="#saveFile" text="Save">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Button>
      <Button layoutX="414.0" layoutY="50.0" mnemonicParsing="false" onAction="#goBack" text="Go Back">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
