<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.helpController">
   <children>
      <SplitPane dividerPositions="0.4999999999999999" prefHeight="400.0" prefWidth="600.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <ListView fx:id="helpList" layoutX="-7.0" prefHeight="400.0" prefWidth="303.0" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <TextArea fx:id="helpArea" layoutX="14.0" layoutY="53.0" prefHeight="318.0" prefWidth="270.0" />
                  <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#showInstructions" text="Show Instructions">
                     <font>
                        <Font name="Calibri" size="14.0" />
                     </font>
                  </Button>
                  <Label layoutX="194.0" layoutY="371.0" text="Dedicated to &quot;Rey&quot;">
                     <font>
                        <Font name="Freestyle Script" size="18.0" />
                     </font>
                  </Label>
                  <Button layoutX="217.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Go Back">
                     <font>
                        <Font name="Calibri" size="14.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
